#include <bits/stdc++.h>
using namespace std;
int prims(int V, vector<vector<int>> adj[]) {
    priority_queue<pair<int,int>, vector<pair<int,int>>, greater<pair<int,int>>> pq;
    vector<bool>visited(V, false);
    pq.push({0,0});
    int sum = 0;
    while(!pq.empty()) {
        auto it = pq.top();
        pq.pop();
        int wt = it.first;
        int u= it.second;
        if(visited[u] == true) continue;
        visited[u] = true;
        sum+=wt;
        for(auto it : adj[u]) {
            int v = it[0];
            int edgeW = it[1];
            if(!visited[v]) {
                pq.push({edgeW, v});
            }
